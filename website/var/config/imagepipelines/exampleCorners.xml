<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
  <items>
    <item>
      <method>cover</method>
      <arguments>
        <width>275</width>
        <height>150</height>
        <positioning>center</positioning>
        <doNotScaleUp>1</doNotScaleUp>
      </arguments>
    </item>
    <item>
      <method>addOverlay</method>
      <arguments>
        <path>/website/static/img/logo-overlay.png</path>
        <x>10</x>
        <y>10</y>
        <origin>top-left</origin>
        <alpha>100</alpha>
        <composite>COMPOSITE_DEFAULT</composite>
      </arguments>
    </item>
    <item>
      <method>roundCorners</method>
      <arguments>
        <width>10</width>
        <height>10</height>
      </arguments>
    </item>
  </items>
  <name>exampleCorners</name>
  <description/>
  <format>SOURCE</format>
  <quality>90</quality>
  <highResolution>0</highResolution>
</zend-config>
