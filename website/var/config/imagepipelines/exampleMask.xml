<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
  <items>
    <item>
      <method>cover</method>
      <arguments>
        <width>275</width>
        <height>150</height>
        <positioning>center</positioning>
        <doNotScaleUp>1</doNotScaleUp>
      </arguments>
    </item>
    <item>
      <method>applyMask</method>
      <arguments>
        <path>/website/static/img/mask-example.png</path>
      </arguments>
    </item>
  </items>
  <name>exampleMask</name>
  <description/>
  <format>SOURCE</format>
  <quality>90</quality>
  <highResolution>0</highResolution>
</zend-config>
